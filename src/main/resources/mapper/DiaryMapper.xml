<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plantee.mapper.DiaryMapper">
	<select id="list" resultType="hashmap">
      select * from diary_view where uid=#{uid};
   </select>
   <select id="read" resultType="hashmap">
      select * from diary_view where diary_id=#{diary_id};
   </select>
   <insert id="insert">
		insert into diary(user_id, plant_name, image, category, contents)
		value (#{user_id}, #{plant_name}, #{image}, 8, #{contents});
   </insert>
   <update id="update">
   
   </update>
   <delete id="delete">
   		delete from diary where diary_id=#{diary_id};
   </delete>
</mapper>